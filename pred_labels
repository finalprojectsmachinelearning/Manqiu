function pred_labels=predict_labels(train_inputs,train_labels,test_inputs)


[b,se,pval,inmodel,stats,nextstep,history] = stepwisefit(train_inputs,train_labels(:,1));

train_reduced=train_inputs(:,inmodel);
test_reduced=test_inputs(:,inmodel);
pred_labels=nn(train_reduced,train_labels,test_reduced);

end


